<?xml version="1.0" encoding="utf-8"?>
<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_column="10"
    android:orientation="horizontal"
    android:rowCount="7"
    android:columnCount="6"
    android:layout_gravity="bottom"
    android:id="@+id/mybtnGrid"
    >


    <Button
        android:id="@+id/clearAll2"
        android:layout_width="53dp"
        android:layout_height="wrap_content"
        android:layout_row="4"
        android:layout_column="0"
        android:onClick="listenUp"
        android:text="CE" />

    <Button
        android:onClick="listenUp"
        android:id="@+id/one"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="3"
        android:layout_column="1"
        android:text="1" />

    <Button
        android:onClick="listenUp"
        android:id="@+id/two"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="3"
        android:layout_column="0"
        android:text="2" />

    <Button
        android:onClick="listenUp"
        android:id="@+id/Par1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="3"
        android:layout_column="4"
        android:layout_weight="1"
        android:text="(" />

    <Button
        android:onClick="listenUp"
        android:id="@+id/three"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="2"
        android:layout_column="0"
        android:text="3" />

    <Button
        android:onClick="listenUp"
        android:id="@+id/four"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="1"
        android:layout_column="0"
        android:text="4" />

    <Button
        android:onClick="listenUp"
        android:id="@+id/nine"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="2"
        android:layout_column="2"
        android:text="9" />

    <Button
        android:onClick="listenUp"
        android:id="@+id/six"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="2"
        android:layout_column="1"
        android:text="6" />

    <Button
        android:onClick="listenUp"
        android:id="@+id/seven"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="0"
        android:layout_column="0"
        android:text="7" />

    <Button
        android:onClick="listenUp"
        android:id="@+id/eight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="0"
        android:layout_column="1"
        android:text="8" />

    <Button
        android:onClick="listenUp"
        android:id="@+id/zero"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="3"
        android:layout_column="2"
        android:text="0" />

    <Button
        android:id="@+id/minus"
        android:onClick="listenUp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="0"
        android:layout_column="2"
        android:text="-" />

    <Button
        android:onClick="listenUp"
        android:id="@+id/plus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="1"
        android:layout_column="2"
        android:text="+" />

    <Button
        android:onClick="listenUp"
        android:id="@+id/multiply"
        android:layout_width="52dp"
        android:layout_height="wrap_content"
        android:layout_row="0"
        android:layout_column="3"
        android:text="*" />

    <Button
        android:onClick="listenUp"
        android:id="@+id/five"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="1"
        android:layout_column="1"
        android:text="5" />

    <Button
        android:onClick="listenUp"
        android:id="@+id/division"
        android:layout_width="52dp"
        android:layout_height="wrap_content"
        android:layout_row="1"
        android:layout_column="3"
        android:text="/" />

    <Button
        android:onClick="listenUp"
        android:id="@+id/clearAll"
        android:layout_width="53dp"
        android:layout_height="wrap_content"
        android:layout_row="0"
        android:layout_column="5"
        android:text="CE" />

    <Button
        android:onClick="listenUp"
        android:id="@+id/equal"
        android:layout_width="52dp"
        android:layout_height="wrap_content"
        android:layout_row="3"
        android:layout_column="3"
        android:text="=" />

    <Button
        android:onClick="listenUp"
        android:id="@+id/exp"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:layout_row="2"
        android:layout_column="3"
        android:text="^" />

    <Button
        android:onClick="listenUp"
        android:id="@+id/log"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="1"
        android:layout_column="4"
        android:text="LOG" />

    <Button
        android:onClick="listenUp"
        android:id="@+id/squareRoot"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="2"
        android:layout_column="4"
        android:text="\u221A" />

    <Button
        android:onClick="listenUp"
        android:id="@+id/par2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_row="0"
        android:layout_column="4"
        android:text=")" />
</GridLayout>